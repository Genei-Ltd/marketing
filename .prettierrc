{
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-multiline-arrays"
	],
	"useTabs": true,
	"tabWidth": 4,
	"semi": false,
	"printWidth": 120,
	"bracketSpacing": true,
	"arrowParens": "always",
	"svelteBracketNewLine": false,
	"objectCurlySpacing": true,
	"multilineArraysWrapThreshold": 1,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
